<!DOCTYPE html>
<html lang="de">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Site-Loader</title>
  <script defer src="https://unpkg.com/bcryptjs@2.4.3/dist/bcrypt.js"></script>
</head>

<body>
  <style>
    *,
    *::before,
    *::after {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }
  </style>
  <script>
    async function getData() {
      let res = await fetch("/get").then(res => res.json()).then(data => data)
    }
    getData()
  </script>
</body>

</html>